{
    "name": "two-thirds/eloquent-traits",
    "description": "",
    "keywords": [],
    "homepage": "https://www.gitlab.com/two-thirds/eloquent-traits.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Hoff",
            "email": "jhoff@twothirds.io"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "5.*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.6",
        "php-mock/php-mock-phpunit": "^2.1",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "~7.0",
        "two-thirds/artisan-anywhere": "^1.0",
        "two-thirds/laravel-test-suite": "^1.0"
    },
    "suggest": {
        "anthonymartin/geo-location": "Required if you want to test GeoLocation functionality with SqlLite ( TestsSqLiteGeoLocation )"
    },
    "config": {
        "secure-http": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "TwoThirds\\EloquentTraits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwoThirds\\Testing\\": "tests"
        }
    }
}
